{
  "name": "nextjs-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --turbo --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "analyze": "ANALYZE_BUNDLE=true pnpm run build",
    "build:test": "NODE_ENV=test pnpm with-env:test next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,mdx,json}\"",
    "start:test": "NODE_ENV=test pnpm with-env:test next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.19",
    "@ai-sdk/react": "^3.0.71",
    "@creatorem/ai-assistant-react": "workspace:*",
    "@creatorem/ai-assistant-react-markdown": "workspace:*",
    "@creatorem/ai-react": "workspace:*",
    "@creatorem/ai-sdk": "workspace:*",
    "@creatorem/ai-store": "workspace:*",
    "@creatorem/ai-chat": "workspace:*",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "ai": "^6.0.50",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "motion": "^12.29.2",
    "next": "16.1.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "tw-shimmer": "^0.4.3",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.28.0"
